import java.util.*;
public class  fibonacci{
    public static void main(String[] args){
        System.out.println("Enter n");
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int res = fibonacci(n);
        System.out.print(res+" ");
    }
    static int fibonacci(int n){
        if((n==0) || (n==1)){
            return 1;
        }
         return fibonacci(n-1) + fibonacci(n-2); 
    }
}
